{
  "manifest_version": 3,
  "name": "SeerSync",
  "description": "Browser-native AI-enabled real-time communication extension.",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/ssys_logo.png",
      "48": "icons/ssys_logo.png",
      "128": "icons/ssys_logo.png"
    }
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "identity"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/index.ts-DnRJZHcy.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/index.ts-DnRJZHcy.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
